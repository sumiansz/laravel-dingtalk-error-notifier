{
    "name": "sumiansz/laravel-dingtalk-error-notifier",
    "description": "DingTalk chat error notifier for Laravel.",
    "keywords": [
        "ding chat",
        "error notifier",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "^5.5 | ^6.0",
        "wangju/ding-notice": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Sumian\\DingtalkErrorNotifier\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sumian\\DingtalkErrorNotifier\\DingtalkErrorNotifierServiceProvider"
            ]
        }
    }
}
